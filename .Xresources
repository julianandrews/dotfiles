! Misc settings
URxvt.depth:                32
URxvt.geometry:             90x30
URxvt.urgentOnBell:         true
URxvt.visualBell:           false
URxvt.loginShell:           false
URxvt.saveLines:            8192
URxvt.internalBorder:       0
URxvt.lineSpace:            0
URxvt.scrollBar:            false

! Fonts
URxvt.allow_bold:           true
URxvt.font: xft:DejaVuSansM Nerd Font Mono:style=Regular:size=11
URxvt.boldFont: xft:DejaVuSansM Nerd Font Mono:style=Bold:size=11
URxvt.italicFont: xft:DejaVuSansM Nerd Font Mono:style=Oblique:size=11
URxvt.letterSpace:          -1
URxvt.intensityStyles:      true

! Perl extensions
URxvt.urlLauncher:          /usr/bin/xdg-open
URxvt.matcher.button:       3

! Cursor/Pointer
URxvt.cursorBlink:          false
URxvt.cursorUnderline:      false
URxvt.matcher.rend.0:       Uline fg5
URxvt.pointerBlank:         false

URxvt*scrollTtyOutput:      false
URxvt*scrollWithBuffer:     true
URxvt*scrollTtyKeypress:    true

